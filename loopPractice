#include <stdio.h>

int main(){
    
    //variables
    int num1;
    int candidate = 0;
    int vote = 0;
    int cand1 = 0;
    int cand2 = 0;
    int cand3 = 0;
    int cand4 = 0;
    int cand5 = 0;
    int cand6 = 0;
    int cand7 = 0;
    int cand8 = 0;
    int cand9 = 0;
    int cand10 = 0;

    do
    {
        printf("Please enter an integer number between 0 thru 90 (inclusively): ");
        scanf_s("%d", &num1);
    } while (num1 < 0 || num1 > 90);
   
   printf("Very good! Please vote for one of the following numbers\n");

   int num2 = num1;

   while (candidate < 10)
   {
       printf("%  d", num2++);
       candidate = candidate +1;
   }

   int num3 = (num1 + 9);

   printf("\nVote for your favorite number between %d thru %d (negative number to end): ", num1, num3);
   scanf_s("%d", &vote);

   while (vote < num1 || vote > num3)
   {
       printf("(%d) is not in the range [%d-%d]", vote, num1, num3);
       printf("\nVote for your favorite number between %d thru %d (negative number to end): ", num1, num3);
       scanf_s("%d", &vote);
   }

   if (vote == num1)
   {
       cand1 = cand1 + 1;
   }
   else if (vote == num1 + 1)
   {
       cand2 = cand2 + 1;
   }
   else if (vote == num1 + 2)
   {
       cand3 = cand3 + 1;
   }
   else if (vote == num1 + 3)
   {
       cand4 = cand4 + 1;
   }
   else if (vote == num1 + 4)
   {
       cand5 = cand5 + 1;
   }
   else if (vote == num1 + 5)
   {
       cand6 = cand6 + 1;
   }
   else if (vote == num1 + 6)
   {
       cand7 = cand7 + 1;
   }
   else if (vote == num1 + 7)
   {
       cand8 = cand8 + 1;
   }
   else if (vote == num1 + 8)
   {
       cand9 = cand9 + 1;
   }
   else if (vote == num3)
   {
       cand10 = cand10 + 1;
   }
   else if (vote > 0)
   {
       printf("The talley of are %d %d %d", cand1, cand2, cand3);
   }
   
        
}   //end main





#include <stdio.h>

int main(){
    
    //variables
    int num1;
    int candidates = 0;
    int vote = 0;
    int cand1 = 0;
    int cand2 = 0;
    int cand3 = 0;
    int cand4 = 0;
    int cand5 = 0;
    int cand6 = 0;
    int cand7 = 0;
    int cand8 = 0;
    int cand9 = 0;
    int cand10 = 0;

    do
    {
        printf("Please enter an integer number between 0 thru 90 (inclusively): ");
        scanf_s("%d", &num1);
    } while (num1 < 0 || num1 > 90);
   
   printf("Very good! Please vote for one of the following numbers\n");

   int num2 = num1;

   while (candidates < 10)
   {
       printf("%  d", num2++);
       candidates = candidates +1;
   }

   int num3 = (num1 + 9);

   while (vote < num1 || vote > num3)
   {
       printf("\nVote for your favorite number between %d thru %d (negative number to end): ", num1, num3);
       scanf_s("%d", &vote);
       //printf("(%d) is not in the range [%d-%d]", vote, num1, num3);

       

       if (vote == num1)
       {
           cand1 = cand1 + 1;
       }
       else if (vote == num1 + 1)
       {
           cand2 = cand2 + 1;
       }
       else if (vote == num1 + 2)
       {
           cand3 = cand3 + 1;
       }
       else if (vote == num1 + 3)
       {
           cand4 = cand4 + 1;
       }
       else if (vote == num1 + 4)
       {
           cand5 = cand5 + 1;
       }
       else if (vote == num1 + 5)
       {
           cand6 = cand6 + 1;
       }
       else if (vote == num1 + 6)
       {
           cand7 = cand7 + 1;
       }
       else if (vote == num1 + 7)
       {
           cand8 = cand8 + 1;
       }
       else if (vote == num1 + 8)
       {
           cand9 = cand9 + 1;
       }
       else if (vote == num3)
       {
           cand10 = cand10 + 1;
       }
       
   } 

  
   /*else if (vote > 0)
   {
       printf("The talley of are %d %d %d", cand1, cand2, cand3);
   }*/
   
        
}  




#include <stdio.h>

int main(){
    
    //variables
    int num1;
    int candidates = 0;
    int vote = 0;
    int cand1 = 0;
    int cand2 = 0;
    int cand3 = 0;
    int cand4 = 0;
    int cand5 = 0;
    int cand6 = 0;
    int cand7 = 0;
    int cand8 = 0;
    int cand9 = 0;
    int cand10 = 0;

    do
    {
        printf("Please enter an integer number between 0 thru 90 (inclusively): ");
        scanf_s("%d", &num1);
    } while (num1 < 0 || num1 > 90);
   
   printf("Very good! Please vote for one of the following numbers\n");

   int num2 = num1;

   while (candidates < 10)
   {
       printf("%  d", num2++);
       candidates = candidates +1;
   }

   int num3 = (num1 + 9);

   printf("\nVote for your favorite number between %d thru %d (negative number to end): ", num1, num3);
   scanf_s("%d", &vote);

   while (vote >= 0)
   {
       
       while (vote >= num1 || vote <= num3)
       {
           while (vote < num1 || vote > num3)
           {
               printf("(%d) is not in the range [%d-%d]", vote, num1, num3);
               printf("\nVote for your favorite number between %d thru %d (negative number to end): ", num1, num3);
               scanf_s("%d", &vote);
           }            
               if (vote == num1)
               {
                   cand1 = cand1 + 1;
               }
               else if (vote == num1 + 1)
               {
                   cand2 = cand2 + 1;
               }
               else if (vote == num1 + 2)
               {
                   cand3 = cand3 + 1;
               }
               else if (vote == num1 + 3)
               {
                   cand4 = cand4 + 1;
               }
               else if (vote == num1 + 4)
               {
                   cand5 = cand5 + 1;
               }
               else if (vote == num1 + 5)
               {
                   cand6 = cand6 + 1;
               }
               else if (vote == num1 + 6)
               {
                   cand7 = cand7 + 1;
               }
               else if (vote == num1 + 7)
               {
                   cand8 = cand8 + 1;
               }
               else if (vote == num1 + 8)
               {
                   cand9 = cand9 + 1;
               }
               else if (vote == num3)
               {
                   cand10 = cand10 + 1;
               }
                           
               printf("Vote for your favorite number between %d thru %d (negative number to end): ", num1, num3);
               scanf_s("%d", &vote);

                if (vote < 0)
                {
                    printf("The Tallies of the numbers are: \n");
                    printf("Number %d: %d votes.", num1, cand1);
                    printf("\nNumber %d: %d votes.", num1 + 1, cand2);
                    printf("\nNumber %d: %d votes.", num1 + 2, cand3);
                    printf("\nNumber %d: %d votes.", num1 + 3, cand4);
                    printf("\nNumber %d: %d votes.", num1 + 4, cand5);
                    printf("\nNumber %d: %d votes.", num1 + 5, cand6);
                    printf("\nNumber %d: %d votes.", num1 + 6, cand7);
                    printf("\nNumber %d: %d votes.", num1 + 7, cand8);
                    printf("\nNumber %d: %d votes.", num1 + 8, cand9);
                    printf("\nNumber %d: %d votes.", num1 + 9, cand10);
                }
     
        }
   }
        
}   //end main






#include <stdio.h>

int main(){
    
    //variables
    int num1;
    int candidates = 0;
    int vote = 0;
    int cand1 = 0;
    int cand2 = 0;
    int cand3 = 0;
    int cand4 = 0;
    int cand5 = 0;
    int cand6 = 0;
    int cand7 = 0;
    int cand8 = 0;
    int cand9 = 0;
    int cand10 = 0;
    int winning_Number = 0;
    int winning_Votes = 0;


    do
    {
        printf("Please enter an integer number between 0 thru 90 (inclusively): ");
        scanf_s("%d", &num1);
    } while (num1 < 0 || num1 > 90);
   
   printf("Very good! Please vote for one of the following numbers\n");

   int num2 = num1;

   while (candidates < 10)
   {
       printf("%  d", num2++);
       candidates = candidates +1;
   }

   int num3 = (num1 + 9);

   printf("\nVote for your favorite number between %d thru %d (negative number to end): ", num1, num3);
   scanf_s("%d", &vote);

   while (vote >= 0)
   {
       
       while (vote >= num1 || vote <= num3)
       {
           while (vote < num1 || vote > num3)
           {
               printf("(%d) is not in the range [%d-%d]", vote, num1, num3);
               printf("\nVote for your favorite number between %d thru %d (negative number to end): ", num1, num3);
               scanf_s("%d", &vote);
           }            
               if (vote == num1)
               {
                   cand1 = cand1 + 1;
               }
               else if (vote == num1 + 1)
               {
                   cand2 = cand2 + 1;
               }
               else if (vote == num1 + 2)
               {
                   cand3 = cand3 + 1;
               }
               else if (vote == num1 + 3)
               {
                   cand4 = cand4 + 1;
               }
               else if (vote == num1 + 4)
               {
                   cand5 = cand5 + 1;
               }
               else if (vote == num1 + 5)
               {
                   cand6 = cand6 + 1;
               }
               else if (vote == num1 + 6)
               {
                   cand7 = cand7 + 1;
               }
               else if (vote == num1 + 7)
               {
                   cand8 = cand8 + 1;
               }
               else if (vote == num1 + 8)
               {
                   cand9 = cand9 + 1;
               }
               else if (vote == num3)
               {
                   cand10 = cand10 + 1;
               }
                           
               printf("Vote for your favorite number between %d thru %d (negative number to end): ", num1, num3);
               scanf_s("%d", &vote);

                   if (vote < 0)
                   {
                       printf("The Tallies of the numbers are: \n");
                       printf("Number %d: %d votes.", num1, cand1);
                       printf("\nNumber %d: %d votes.", num1 + 1, cand2);
                       printf("\nNumber %d: %d votes.", num1 + 2, cand3);
                       printf("\nNumber %d: %d votes.", num1 + 3, cand4);
                       printf("\nNumber %d: %d votes.", num1 + 4, cand5);
                       printf("\nNumber %d: %d votes.", num1 + 5, cand6);
                       printf("\nNumber %d: %d votes.", num1 + 6, cand7);
                       printf("\nNumber %d: %d votes.", num1 + 7, cand8);
                       printf("\nNumber %d: %d votes.", num1 + 8, cand9);
                       printf("\nNumber %d: %d votes.", num1 + 9, cand10);
                       break;
                   }
     
       }
   }

   if (cand1 > winning_Votes)
   {
        winning_Votes = cand1;
        winning_Number = num1;
   }
       if (cand2 > winning_Votes)
       {
            winning_Votes = cand2;
            winning_Number = num1 + 1;
       }
           if (cand3 > winning_Votes)
           {
                winning_Votes = cand3;
                winning_Number = num1 + 2;
           }
               if (cand4 > winning_Votes)
               {
                   winning_Votes = cand4;
                   winning_Number = num1 + 3;
               }
                   if (cand5 > winning_Votes)
                   {
                       winning_Votes = cand5;
                       winning_Number = num1 + 4;
                   }
                       if (cand6 > winning_Votes)
                       {
                           winning_Votes = cand6;
                           winning_Number = num1 + 5;
                       }
                           if (cand7 > winning_Votes)
                           {
                               winning_Votes = cand7;
                               winning_Number = num1 + 6;
                           }
                               if (cand8 > winning_Votes)
                               {
                                   winning_Votes = cand8;
                                   winning_Number = num1 + 7;
                               }
                                   if (cand9 > winning_Votes)
                                   {
                                       winning_Votes = cand9;
                                       winning_Number = num1 + 8;
                                   }
                                       if (cand10 > winning_Votes)
                                       {
                                           winning_Votes = cand10;
                                           winning_Number = num1 + 9;
                                       }
                                           if (winning_Votes == 0)
                                           {
                                               printf("There were no votes properly casted.");
                                               return 0;
                                           }
   printf("\nThe winner is number %d with %d votes\n", winning_Number, winning_Votes);
        
}   //end main
